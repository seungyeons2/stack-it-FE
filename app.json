{
  "expo": {
    "name": "두둑",
    "slug": "doodook",
    "owner": "yehyeon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-d-app-icon-1500.png",
    "splash": {
      "image": "./src/assets/images/logo-d-app-icon-1500.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.lipeuoo.doodook",
      "buildNumber": "22",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-d-app-icon-1500.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.r8ol7z.doodook"
    },
    "web": {
      "favicon": "./src/assets/images/logo-d-app-icon-1500.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icons/star-filled.png",
          "color": "#F074BA"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "57b0a621-af5d-4605-b6e0-cc46a6c474ec"
      }
    }
  }
}
